	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Docker: Attach to Node",
			"remoteRoot": "/usr/src/app",
			"port": 9299
		},
		{
			"command": "npm run dev",
			"name": "Run npm dev",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": ["<node_internals>/**"],
			"program": "build/index.js",
			"outFiles": ["build/**/*.js"]
		},
		{
			"name": "Docker Nod.js Launch",
			"type": "docker",
			"request": "launch",
			"preLaunchTask": "docker-run: debug",
			"platform": "node"
		},
		{
			"name": "Docker Node.js Launch",
			"type": "docker",
			"request": "launch",
			"preLaunchTask": "docker-run: debug",
			"platform": "node"
		}
	]
}
